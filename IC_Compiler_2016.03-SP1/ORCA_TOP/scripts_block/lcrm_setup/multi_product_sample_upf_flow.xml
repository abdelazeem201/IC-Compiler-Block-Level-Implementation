    </tool_task>
    <tool_task name="vclp_icc">
        <step>rm_icc</step>
        <script_file>$SEV(bscript_dir)/rm_icc_scripts/vc_lp.tcl</script_file>
        <src>outputs_icc</src>
        <dst>outputs_icc</dst>
