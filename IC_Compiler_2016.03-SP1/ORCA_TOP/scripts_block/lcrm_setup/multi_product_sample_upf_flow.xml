        <dst>metal_fill_icc</dst>
        <tool>icc</tool>
    </tool_task>
    <tool_task name="outputs_icc">
        <step>rm_icc</step>
        <script_file>$SEV(bscript_dir)/rm_icc_zrt_scripts/outputs_icc.tcl</script_file>
        <src>signoff_drc_icc</src>
        <dst>outputs_icc</dst>
        <tool>icc</tool>
    </tool_task>
    <tool_task name="place_opt_icc">
