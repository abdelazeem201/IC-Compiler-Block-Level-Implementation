    </tool_task>
    <tool_task name="vclp_rtl">
        <step>rm_dc</step>
        <script_file>$SEV(bscript_dir)/rm_dc_scripts/vc_lp.tcl</script_file>
        <src>dc</src>
        <dst>dc</dst>
        <tool>vc_static</tool>
        <variables>
            <variable name="TEV(VCLP_RUN)" value="RTL"/>
        </variables>
    </tool_task>
